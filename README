  /usr/share/doc/cdlabelgen-4.1.0/ChangeLog
  /usr/share/doc/cdlabelgen-4.1.0/INSTALL
  /usr/share/doc/cdlabelgen-4.1.0/README
  /usr/share/man/man1/cdlabelgen.1.gz

